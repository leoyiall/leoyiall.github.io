  /*统一样式css*/
  *{margin:0;padding:0;}
  body{background:#F1F1F1;}
  /*头部css*/
	#Top{
		width:100%;
		height:500px;
		background:url("../images/fuhua.jpg");
		background-repeat:no-repeat;
	}
	#Top #Nav{width:1000px;height:60px;margin:0 auto;padding-top:30px;}
	#Top #Nav .logo{width:120px;height:60px;float:left;font-size:30px;color:#fff;line-height:60px;text-align:center;font-family:"微软雅黑";margin-left:50px;}
	#Top #Nav .dh{width:520px;height:60px;float:right;
		line-height:60px;
	}
	#Top #Nav .dh ul li{
		height:60px;
		text-align:center;
		float:left;
		font-size:20px;
		font-family:"微软雅黑";
		font-weight: bolder;
		list-style-type: none;
		margin-left:20px;
	}
	#Top #Nav .dh .aa{width:45px;}
	#Top #Nav .dh .bb{width:65px;}
	#Top #Nav .dh .cc{width:80px;}
	#Top #Nav .dh .dd{width:100px;}
	#Top #Nav .dh ul a{
		text-decoration: none;
		color:#fff;
	}
	#Top #Nav .dh ul a:visited{
		color:#fff;
	}
	#Top #Nav .dh ul li:hover{
		border-bottom:5px solid #DE7C0D;
		color:#DE7C0D;
	}
	#Top #Nav .dh ul .on{
		border-bottom:5px solid #DE7C0D;
		color:#DE7C0D;
	}
	/*标语CSS样式*/
	#Top .biaoyu{
		width:350px;
		height:190px;
		margin:0 auto;
		color:#000;
		font-size:20px;
		font-weight: bolder;
		font-family: "微软雅黑";
		line-height:35px;
		margin-top:210px;
		margin-left:720px;

	}
		

	#Top .biaoyu span{
		font-size:43px;
		color:red;
	}
	/*这里是主体CSS样式*/
	#Main{
		width:1000px;
		height:1000px;
		margin:0 auto;
		background:#fff;
		box-shadow: 0px 0px 10px #ccc;
		}
	#Main .fuhua{
		width:1000px;
		height:500px;
		margin:0 auto;
		border:1px solid #000;
	}
	#Main .fuhua .cen{
		margin:10px auto 10px 400px;
	}
	#Main .fuhua .wenben{
		width:950px;
		height:300px;
		margin:0 auto;
		border:1px solid #000;
	}

	/*这里是介绍孵化的CSS*/
	#Con{
	width:950px;
	height:500px;
	margin:0px auto;
	border:1px solid #ddd;
	border-radius:10px;
	box-shadow:0px 0px 7px #ddd;
}
#button2{height:62px; border-radius:5px;width:500px;margin:0 auto;}
#button2 a{color:#000; font-size:16px; font-family:"微软雅黑";
		height:35px; width:80px; display:block; float:left; margin-top:10px; text-align:center;
		line-height:35px; margin-left:15px;cursor: pointer;
		font-weight: bolder;border-radius: 10px;
		box-shadow: 0 0 5px #ccc;
	}
#button2 a.ck{background:url("images/but.png"); color:#fff;background:#1E90CA;}

#content .txt_con{
	width:950px;
	height:420px;
	border:1px solid #000;
	display:none;
}
#content .txt_con .Left{
	width:400px;
	height:400px;
	margin-top:10px;
	float:left;
	border:5px solid #fff;
	box-shadow: 0 0 10px #ccc;
}

#content .txt_con .Right{
	width:540px;
	height:400px;
	margin-left:5px;
	border:1px solid #000;
	margin-top:10px;
	float:left;
}